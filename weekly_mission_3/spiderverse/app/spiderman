class Spiderman {
    constructor (name, age, actor, movies, studio) {
        this.name = name;
        this.age = age;
        this.actor = actor;
        this.movies = movies;
        this.studio = studio;

        getInfo() = () => {
            console.log(`Hey, I'm ${this.actor} from ${this.studio} sudio`)
        }
    }
}

// Esta l√≠nea nos permite exportar nuestra clase
module.exports = Spiderman;